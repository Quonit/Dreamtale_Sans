MenuRun
{
	wd = GetWeekDay
	ap = GetAP
	nowmin = Getnowmin
	nowhour = Getnowhour
	--
	"\n\n\_q"
	--
	"\![*]\q[Say something,altalk]\n"
	"\![*]\q[Speak,altalk]\n"
	"\![*]\q[Talk,altalk]\n"
	"\![*]\q[Ask him to say something,altalk]\n"
	
	if VillageLevel >= 40
	{
		"\![*]\q[Make him say something,altalk]\n"
		"\![*]\q[Force him to talk,altalk]\n"
		"\![*]\q[Force him to talk to you,altalk]\n"
	}
	if blushiemode == "on" && privacyrespecter == "off"
	{
		"\![*]\q[Listen to him,altalk]\n"
		"\![*]\q[Let him talk,altalk]\n"
		"\![*]\q[Hear what he has to say,altalk]\n"
	}
	--
	if lastTalk != ""
	{
		"\![*]\q[Repeat,OnLastTalk]\n\n"
		"\![*]\q[Ask him to repeat himself,OnLastTalk]\n\n"
		"\![*]\q[Ask him to repeat what he last said,OnLastTalk]\n\n"
		"\![*]\q[Ask him to say what he just said,OnLastTalk]\n\n"
		if VillageLevel >= 40
		{
			"\![*]\q[Make him repeat himself,OnLastTalk]\n\n"
			"\![*]\q[What did he just say?,OnLastTalk]\n\n"
			"\![*]\q[Force him to repeat himself,OnLastTalk]\n\n"
		}
	}
	--
	if sittang == "on"
	{
		"\![*]\q[Exit Sitmode,nomoresits]\n"
		--
		if hasblankie == "on"
		{
		"\![*]\q[Take away blanket,takesitblankie]\n"
		}
		else
		{
			"\![*]\q[Give him a blanket,givesleepblankie]\n\n"
		}
	}
	--
	"\![*]\q[ACT,Onyouact] or ask some \![*]\q[QUESTIONS,youtalk]\n/
	\![*]\q[Use some items,itemsmenu]\n/
	\![*]\q[Stats,configmenu]\n/
	\![*]\q[Use some functions,dosomething]\n\n/
	//\![*]\q[Testvariable,testvariable]\n\n/
	\![*]\q[Nevermind,CANCEL]\n\n\n[half]%(wd) %(nowhour):%(nowmin) %(ap), %(month)/%(day)/%(year)\e"
}

Select.altalk
{
	OnAiTalk
}
OnLastTalk
{
	"%(lastTalk)\e"
}

GetWeekDay
{
	if weekday == 0; {"Sun"}
	elseif weekday == 1; {"Mon"}
	elseif weekday == 2; {"Tue"}
	elseif weekday == 3; {"Wed"}
	elseif weekday == 4; {"Thu"}
	elseif weekday == 5; {"Fri"}
	else; {"Sat"}
}

GetAP
{
	if ampm == 0 {"AM"}
	else; {"PM"}
}

Getnowmin
{
	if minute >= 0 && minute <=9; {"0%(minute)"}
	else; {"%(minute)"}
}

Getnowhour
{
	if hour12 >= 0 && hour12 <=9; {"0%(hour12)"}
	else; {"%(hour12)"}
}

