OpenSakuraMenu
{
	lasttalked = 0
	"\b2"
	--
	if nowmode == "Edgybitch"
	{
		GoopMenu
	}
	elseif nowmode == "intodreams"
	{
		OnDreamMenu
	}
	elseif sleepingmode == "on"
	{
		"zzz\w4.\w4.\w4.\w8\n\n* (Wake him up?)\n\n"
		--
		if hasblankie == "off"
		{
			"[*]\q[Give him a blanket,givesleepblankie]\n"
		}
		--
		if nicensadlevel > 50
		{
			"\![*]\q[Tell him something nice,nicewords]\n"
		}
		--
		"\![*]\q[Yes,wakeup]\n/
		\![*]\q[No,letsleep]\e"
	}
	elseif VillageLevel >= 40 && nowmode == "purpleman"
	{
		if RAND(100) < 25
		{
			"\0\s[19]* (. . .)\e"
			"\0\s[19]* (He isn't opening the menu.)\e"
			"\0\s[19]* (He isn't cooperating.)\e"
			"\0\s[19]* (He isn't opening the menu.)\w8\n\n*(Force him to cooperate?)\n\n/
			\![*]\q[* (Ask him what it is.),forcemenuopen]\n/
			\![*]\q[* (Leave him alone.),leavemenualone]\e"
		}
		else
		{
			"\0\s[19]* (. . .)"
			--
			MenuRun
		}
	}
	elseif nicensadlevel <= -20 && punchout >= 30 && nowmode == "purpleman"
	{
		if RAND(100) < 50
		{
			"\0\s[19]* (. . .)\e"
			"\0\s[19]* (He isn't opening the menu.)\e"
			"\0\s[19]* (He isn't cooperating.)\e"
			"\0\s[19]* (He isn't opening the menu.)\w8\n\n*(Force him to cooperate?)\n\n/
			\![*]\q[* (Ask him what it is.),forcemenuopen]\n/
			\![*]\q[* (Leave him alone.),leavemenualone]\e"
		}
		else
		{
			"\0\s[19]* (. . .)"
			--
			MenuRun
		}
	}
	elseif sittang == "on"
	{
		"\0\s[1]\i[7]\i[11]What do you need?"
		"\0\s[1]\i[7]* (. . .)"
		"\0\s[1]\i[7]\i[14]hm?"
		"\0\s[1]\i[11]\i[14]What do you want?"
		--
		MenuRun
	}
	else
	{
		if nicensadlevel <= -10
		{
			"%(annoyed)What do you want...?"
			"%(annoyed)What?"
			"\0\s[0]\i[12]...?"
		}
		elseif blushiemode == "on" && privacyrespecter == "off"
		{
			"\0\s[6]What do you need %(username)?"
			"\0\s[20]What is it you need?"
		}
		elseif nicensadlevel >= 15
		{
			"\0\s[0]\i[14]\i[15]Yeah %(username)?"
			"%(surface0)Do you need something?"
			"%(surface0)What is it?"
			"%(surface0)What do you need?"
		}
		else
		{
			"%(surface0)What do you need?"
			"%(surface0)* (. . .)"
			"%(surface0)hm?"
		}
		--
		MenuRun
	}
}


